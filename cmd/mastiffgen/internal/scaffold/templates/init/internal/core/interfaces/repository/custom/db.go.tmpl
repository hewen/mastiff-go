// Package custom provides custom queries for the database.
package custom

import (
	"github.com/hewen/mastiff-go/store"
)

// New CustomQueries returns a new instance of CustomQueries.
func New(db *store.DB) *CustomQueries {
	return &CustomQueries{db: db}
}

// nolint
// CustomQueries is a struct that holds the database connection and custom queries.
type CustomQueries struct {
	db *store.DB
}
